{% extends 'base-compte.html.twig' %}

{% block title %}Hello ComptesController!{% endblock %}

{% block body %}
<section class="ls section_padding_top_150 section_padding_bottom_150 colum_padding_30">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-md-12 col-lg-12">
                <article class="post event-item side-item side-sm content-padding with_shadow">
                    <div class="ds gradient lightened with_padding">
                        <div class="widget widget_search">
                            <h3 class="widget-title">Search</h3>
                            <form method="get" class="searchform" action="./">
                                <div class="form-group margin_0"> <label class="sr-only"
                                        for="widget-search">Search for:</label> <input id="widget-search"
                                        type="text" value="" name="search" class="form-control"
                                        placeholder="Search here"> </div> <button type="submit"
                                    class="theme_button no_bg_button">Search</button>
                            </form>
                        </div>
                    </div>
                </article>
                 <article class="post event-item side-item side-sm content-padding with_shadow">
                     <div class="row">
                        <div class="col-xs-12 text-center text-sm-left">
            
                                <div class="toppadding_5"></div>
                               
                              {{ form_start(form) }}
                                <div method="post" class=" row columns_margin_bottom_20" >
                                    <div class="col-xs-12 col-sm-12">
                                        <div class="form-group"> {{ form_widget(form) }} </div>
                                    </div>    
                                     <div class="col-xs-12 col-sm-12">
                                        <div class="form-group"> <button type="submit" id="contact_form_submit" name="" class="theme_button color1 min_width_button margin_0">Enregistrer</button> </div>
                                    </div>                                     
                                    
                                </div>
                                {{ form_end(form) }}

                            </div>
                        </div>
                </article>
                 {% for espace in espaces %}
                <article class="post event-item side-item side-sm content-padding with_shadow">
                    <div class="row">
                            <div class="col-sm-7">
                            <div class="item-content">
                                <div class="entry-header">
                                    <div class="entry-meta inline-content with_dividers highlight"> <span>
                                            <time datetime="2017-10-03T08:50:40+00:00">{{espace.dateEcheance}}</time>
                                        </span> </div>
                                    <h3 class="entry-title"> <a href="{{path('app_espaces_show',{'id':espace.id})}}">{{espace.noms}}</a> </h3>
                                </div>
                                <div class="entry-meta small-text content-justify v-spacing v-center">
                                        <div class="darklinks">
										<a href="blog-single-right.html#comments">Lien: <span class="count">www.drepaussd.com  </span></a><a href="blog-single-right.html#comments">Num√©ro: <span class="count"> #911#11#</span></a> </div>
										<div class="share-buttons"> <span class="grey rightmargin_10">PARTAGER:</span> <a class="social-icon color-icon border-icon rounded-icon socicon-facebook" href="#" title="Facebook"></a> <a class="social-icon color-icon border-icon rounded-icon socicon-twitter" href="#" title="Twitter"></a>											<a class="social-icon color-icon border-icon rounded-icon socicon-googleplus" href="#" title="Google Plus"></a> <a class="social-icon color-icon border-icon rounded-icon socicon-instagram" href="#" title="Instagram"></a> </div>
										
								</div>
                            </div>
                        </div> 
                    </div>
                </article>
                {% endfor %}
                <div class="row topmargin_60">
                    <div class="col-sm-12 text-center">
                        <ul class="pagination">
                            <li><a href="#"><span class="sr-only">Prev</span><i class="fa fa-angle-left"
                                        aria-hidden="true"></i></a></li>
                            <li class="active"><a href="#">1</a></li>
                            <li><a href="#">2</a></li>
                            <li><a href="#">3</a></li>
                            <li><a href="#">4</a></li>
                            <li><a href="#">5</a></li>
                            <li><a href="#"><span class="sr-only">Next</span><i class="fa fa-angle-right"
                                        aria-hidden="true"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--eof .col-sm-8 (main content)-->
            <!-- sidebar -->
            
            <!-- eof aside sidebar -->
        </div>
    </div>
</section>
