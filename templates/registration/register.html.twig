{% extends 'base.html.twig' %}

{% block body %}
<section id="contact-form" class="ds gradient lightened section_padding_top_150 section_padding_bottom_150">
    <div class="skew-overlay right"></div>
    <div class="container">
        <div class="row">
            <div class="col-xs-12 text-center text-sm-left">
                <h2 class="section_header">Inscrition</h2>
                <p class="under_heading grey">Vivez une expériance unique</p>
                <div class="toppadding_5"></div>
                {{ form_start(registrationForm) }}
                <div  class=" row columns_margin_bottom_20" >
                 
                     <div class="col-xs-12 col-sm-4">
                        <div class="form-group"> <label for="registration_form_email">E-mail</label> {{ form_widget(registrationForm.email) }} </div>
                    </div>
                    <div class="col-xs-12 col-sm-4">
                        <div class="form-group"> <label for="registration_form_prenom">Prénom</label> {{ form_widget(registrationForm.prenom) }} </div>
                    </div>
                    <div class="col-xs-12 col-sm-4">
                        <div class="form-group"> <label for="registration_form_nom">Nom</label> {{ form_widget(registrationForm.nom) }} </div>
                    </div>
                    <div class="col-xs-12 col-sm-4">
                        <div class="form-group"> <label for="registration_form_telephones">Téléphone</label> {{ form_widget(registrationForm.telephones) }} </div>
                    </div>
                    <div class="col-xs-12 col-sm-4">
                        <div class="form-group"> <label for="registration_form_plainPassword">Mot de passe</label> {{ form_widget(registrationForm.plainPassword) }} </div>
                    </div>
                    
                    <div class="col-xs-12">
                        <div class="contact-form-submit"> <button type="submit" id="contact_form_submit" name="" class="theme_button color1 min_width_button margin_0">Connexion</button> </div>
                    </div>
                    {{ form_end(registrationForm) }}
                </form>
            </div>
        </div>
    </div>
</section>
<section id="testimonials" class="ls section_padding_top_140">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 margin_0">
                <div class="row testimonials-two-owls">
                    <div class="col-xs-12 col-sm-7 col-lg-6 col-sm-push-5 col-lg-push-6 text-center text-sm-left">
                        <div class="owl-carousel tml-content bottom-nav" data-responsive-lg="1" data-responsive-md="1" data-responsive-sm="1" data-nav="true">
                            <blockquote> <cite>Robert Oliphant / Businessman</cite>
                                <p>Great office. Very professional and helpful. Finwiz have been such a great asset to myself and my family.</p>
                            </blockquote>
                            <blockquote> <cite>Hugo Dukes / Investor</cite>
                                <p>Without a doubt the most beneficial decision I've made. Cannot say enough great things about Finwiz.</p>
                            </blockquote>
                            <blockquote> <cite>Tamra Loucks / Student</cite>
                                <p>Very professional and helpful people. They respond to my questions and emails very promptly.</p>
                            </blockquote>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-5 col-lg-6 col-sm-pull-7 col-lg-pull-6 bottommargin_0">
                        <div class="owl-carousel tml-avatars" data-responsive-lg="1" data-responsive-md="1" data-responsive-sm="1" data-animate-in="fadeIn" data-animate-out="fadeOut" data-mouse-drag="false" data-touch-drag="false">
                            <div class="avatar"> <img src="{{domaine}}/util/images/testimonials/01.png" alt=""> </div>
                            <div class="avatar"> <img src="{{domaine}}/util/images/testimonials/02.png" alt=""> </div>
                            <div class="avatar"> <img src="{{domaine}}/util/images/testimonials/03.png" alt=""> </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
{% endblock %}