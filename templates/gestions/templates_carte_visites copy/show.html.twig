{% extends 'base.html.twig' %}

{% block title %}TemplatesCarteVisites{% endblock %}

{% block body %}
    <h1>TemplatesCarteVisites</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ templates_carte_visite.id }}</td>
            </tr>
            <tr>
                <th>Titres</th>
                <td>{{ templates_carte_visite.titres }}</td>
            </tr>
            <tr>
                <th>Descriptions</th>
                <td>{{ templates_carte_visite.descriptions }}</td>
            </tr>
            <tr>
                <th>Visibilites</th>
                <td>{{ templates_carte_visite.visibilites ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Commandes</th>
                <td>{{ templates_carte_visite.commandes }}</td>
            </tr>
